{
  "name": "express-crud",
  "version": "1.0.0",
  "description": "Simple REST API CRUD using Express",
  "main": "src/main.ts",
  "author": "Mathias Artur Schulz",
  "license": "MIT",
  "engines": {
    "node": ">=22.11.0",
    "yarn": ">=1.22.22"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "dev": "ts-node-dev --respawn ./src/main.ts",
    "build": "yarn test && rimraf ./dist && tsc",
    "start": "yarn build && node dist/main.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration": "yarn typeorm migration:run -- -d ./src/core/data-source.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  }
}
